@using WebBanThucAnNhanh.Models
@model List<LOAI>
<style>


    .div-cate {
        font-weight: 500;
        text-transform: uppercase;
    }
</style>
<ul style="        list-style-type: none;  padding: 0;">
    <li class="div-cate">
        <a href="@Url.Action("Index","Home")">Tất cả</a>
    </li>
    @{
        foreach (var item in Model)
        {
            
            <li class="div-cate">
                @Html.ActionLink(item.TENLOAI, "Index", "Home", new { loai = item.MALOAI.Trim() }, "")
            </li>

        }
    }
</ul>
